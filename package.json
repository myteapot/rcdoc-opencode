{
  "name": "rcode-docs",
  "description": "A doc project for Right Code",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:build": "vuepress-vite build src",
    "docs:build:cf": "vuepress-vite build src && mkdir -p src/.vuepress/output/docs && mv src/.vuepress/dist/* src/.vuepress/output/docs/",
    "docs:clean-dev": "vuepress-vite dev src --clean-cache",
    "docs:dev": "vuepress-vite dev src",
    "docs:update-package": "yarn dlx vp-update"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-slimsearch": "^2.0.0-rc.122",
    "sass-embedded": "^1.97.1",
    "vue": "^3.5.26",
    "vuepress": "2.0.0-rc.26",
    "vuepress-theme-hope": "2.0.0-rc.102"
  },
  "packageManager": "yarn@4.12.0"
}
